{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": [
      "@env"
    ],
    "paths": {
      "@/*": [
        "./app/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@/features/*": [
        "./app/features/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "app.config.js"
, "app/hooks/useDeleteAccount"  ]
}
